
@{
    ViewBag.Title = "Gallery";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

<!-- bradcam_area_start -->
<div class="bradcam_area breadcam_bg_2 overlay">
    <h3>Gallery</h3>
</div>

